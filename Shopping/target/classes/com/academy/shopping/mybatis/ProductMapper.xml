<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Product">

	<insert id="insert" parameterType="Product">
		insert into product(product_id,brand,price,discount,memo,detail,product_img,subcategory_id) 
		values(seq_product.nextval,#{brand},#{price},#{discount},#{memo},#{detail},#{product_img},#{subcategory.subcategory_id})
	</insert>

	
</mapper> 